[sub_resource type="GDScript" id=1]

script/source = "extends Node2D\nvar zooming = false\nvar countdown = 0\n\nfunc _ready():\n    get_node(\"Buttons/play_button\").connect(\"pressed\",self,\"_on_play_pressed\")\n    get_node(\"Buttons/exit_button\").connect(\"pressed\",self,\"_on_exit_pressed\")\n    set_process(true)\n\nfunc _on_play_pressed():\n\tget_node(\"background\").blink()\n\tget_node(\"seagull_sounds\").play(\"mewy 1\")\n\t\nfunc _on_exit_pressed():\n\tget_node(\"seagull_sounds\").play(\"mewy 2\")\n\tget_tree().quit()\n\t\nfunc _process(delta):\n\tif(not zooming):\n\t\tif(get_node(\"background\").start_zoom):\n\t\t\tstart_animation()\n\telif(not get_node(\"AnimationPlayer\").is_playing()):\n\t\tif(countdown > 0.5):\n\t\t\tget_tree().change_scene(\"res://level1.tscn\");\n\t\telse:\n\t\t\tcountdown += delta\n\n\nfunc start_animation():\n\tget_node(\"AnimationPlayer\").play(\"zoom\");\n\tzooming = true;\n\t"

